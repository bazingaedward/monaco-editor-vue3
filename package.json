{
  "name": "monaco-editor-vue3",
  "version": "1.0.3",
  "keywords": [
    "Vue3",
    "monaco-editor"
  ],
  "author": "Edward Qiu",
  "license": "MIT",
  "homepage": "https://bazingaedward.github.io/monaco-editor-vue3/",
  "bugs": {
    "url": "https://github.com/bazingaedward/monaco-editor-vue3/issues"
  },
  "repository": {
    "url": "bazingaedward/monaco-editor-vue3",
    "type": "git"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dist/*.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "start": "pnpm dev",
    "build": "vite build",
    "lint": "biome check . && eslint .",
    "lint:fix": "biome check --write . && eslint --fix .",
    "lint:biome": "biome check .",
    "lint:biome:fix": "biome check --write .",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "format": "biome format --write .",
    "test": "vitest",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git add . && pnpx git-cz",
    "docs:dev": "vitepress dev docs",
    "docs:build": "pnpm build && vitepress build docs",
    "docs:build:fast": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "build:docs": "./scripts/build-docs.sh",
    "deploy": "./scripts/deploy.sh"
  },
  "packageManager": "pnpm@8.15.1",
  "peerDependencies": {
    "monaco-editor": ">= 0.25.0 < 1",
    "vue": "^3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/compiler-sfc": "^3.4.15",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.4",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^15.2.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.33",
    "sass": "^1.70.0",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^0.17.6",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.5.4",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
}
